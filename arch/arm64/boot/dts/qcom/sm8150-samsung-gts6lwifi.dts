/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/input/gpio-keys.h>
#include <dt-bindings/pinctrl/qcom,pmic-gpio.h>
#include <dt-bindings/regulator/qcom,rpmh-regulator.h>
#include "sm8150.dtsi"
#include "pm8009.dtsi"
#include "pm8150.dtsi"
#include "pm8150l.dtsi"

/delete-node/ &tz_mem;
/delete-node/ &rmtfs_mem;
/delete-node/ &adsp_mem;
/delete-node/ &mpss_mem;
/delete-node/ &venus_mem;
/delete-node/ &slpi_mem;
/delete-node/ &ipa_fw_mem;
/delete-node/ &ipa_gsi_mem;
/delete-node/ &gpu_mem;
/delete-node/ &spss_mem;
/delete-node/ &cdsp_mem;

/ {
	model = "Samsung Tab S6";
	compatible = "samsung,gts6lwifi", "qcom,sm8150-mtp", "qcom,sm8150";
	qcom,msm-id = <339 0x20000>;
    qcom,board-id = <0x00010008 0x00000002>;

	aliases {
		serial0 = &uart2;
		display0 = &framebuffer;
		hsuart0 = &uart13;
		wifi0 = &wifi;
	};

	chosen {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		framebuffer: framebuffer@9c400000 {
			compatible = "simple-framebuffer";
			reg = <0 0x9c400000 0 0x2000000>;
			status = "okay";
			width = <2560>;
			height = <1600>;
			stride = <(2560 * 4)>;
			format = "a8r8g8b8";
			clocks =	<&gcc GCC_DISP_HF_AXI_CLK>,
						<&gcc GCC_DISP_SF_AXI_CLK>;
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&vol_up_pin_a>;

		vol_up {
			label = "volume_up";
			gpios = <&pm8150l_gpios 12 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_VOLUMEUP>;
			debounce-interval = <15>;
			linux,can-disable;
		};
	};

	reserved-memory {
		tz_mem: memory@86200000 {
			no-map;
			reg = <0x0 0x86200000 0x0 0x5fe4000>;
		};

		adsp_mem: memory@99300000 {
			no-map;
			reg = <0x0 0x99300000 0x0 0x2700000>;
		};

		mpss_mem: memory@8d800000 {
			no-map;
			reg = <0x0 0x8d800000 0x0 0xa000000>;
		};

		venus_mem: memory@9bf00000 {
			no-map;
			reg = <0x0 0x9bf00000 0x0 0x500000>;
		};

		slpi_mem: memory@8c400000 {
			no-map;
			reg = <0x0 0x8c400000 0x0 0x1400000>;
		};

		ipa_fw_mem: memory@8c1e4000 {
			no-map;
			reg = <0x0 0x8c1e4000 0x0 0x10000>;
		};

		ipa_gsi_mem: memory@8c1f4000 {
			no-map;
			reg = <0x0 0x8c1f4000 0x0 0x5000>;
		};

		gpu_mem: memory@8c1fe000 {
			no-map;
			reg = <0x0 0x8c1fe000 0x0 0x2000>;
		};

		spss_mem: memory@97800000 {
			no-map;
			reg = <0x0 0x97800000 0x0 0x100000>;
		};

		cdsp_mem: memory@97900000 {
			no-map;
			reg = <0x0 0x97900000 0x0 0x1a00000>;
		};

		cont_splash_mem: memory@9c400000 {
			reg = <0x0 0x9c400000 0x0 0x2000000>;
            label = "cont_splash_region";
		};

		cdsp_sec_mem: memory@a4c00000 {
			reg = <0x0 0xa4c00000 0x0 0x1400000>;
			no-map;
		};

		ramoops@a1300000 {
			compatible = "ramoops";
			reg = <0x0 0xc1300000 0x0 0x800000>;
			console-size = <0x400000>;
			ecc-size = <0>;
			ftrace-size = <0>;
			pmsg-size = <0x200000>;
			record-size = <0>;
			no-map;
		};

		rmtfs_mem: memory@fe101000 {
			compatible = "qcom,rmtfs-mem";
			reg = <0 0xfe101000 0 0x300000>;
			no-map;

			qcom,client-id = <1>;
			qcom,vmid = <15>;
		};
	};

	cs48l33_dcvdd_1p2: regulator_dcvdd {
		compatible = "regulator-fixed";
		regulator-name = "regulator-dcvdd-supply";
		status = "ok";
		regulator-min-microvolt = <1200000>;
		regulator-max-microvolt = <1200000>;
		gpio = <&tlmm 60 0>; /* VOICE_LDO_ENABLE */
		enable-active-high;
		startup-delay-us = <10000>;
	};

	wacom_ldo_en: wacom_ldo_en {
		compatible = "regulator-fixed";
		regulator-name = "wacom_3.3v";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&tlmm 68 0>;
		enable-active-high;
		regulator-boot-on;
	};

	pogo_kbd_ldo_en: pogo_kbd_ldo_en {
		compatible = "regulator-fixed";
		regulator-name = "pogo_kbd_ldo_en_3.3v";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&tlmm 65 0>;
		enable-active-high;
	};

	lcd_ldo_en: fixed_reg_octavdd {
		compatible = "regulator-fixed";
		status = "okay";
		regulator-name = "octa-vdd";
		gpio = <&tlmm 113 0>; /* LCD_LDO_EN */
		enable-active-high;
		regulator-boot-on;
	};

	vph_pwr: vph-pwr-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vph_pwr";
		regulator-min-microvolt = <3700000>;
		regulator-max-microvolt = <3700000>;
	};

	/* S4 is modeled as a fixed regulator in vendor sources */
	vreg_s4a_1p8: pm8150-s4-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vreg_s4a_1p8";

		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;

		regulator-always-on;
		regulator-boot-on;

		vin-supply = <&vph_pwr>;
	};
};

